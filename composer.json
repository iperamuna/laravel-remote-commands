{
    "name": "iperamuna/laravel-remote-commands",
    "description": "Remote SSH Access for the Laravel Framework via phpseclib.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Indunil Peramuna",
            "email": "iperamuna@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "phpseclib/phpseclib": "^3.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelRemoteCommands\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRemoteCommands\\LaravelRemoteCommandsServiceProvider"
            ]
        }
    }
}
